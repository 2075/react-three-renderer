{
  "name": "react-three-renderer",
  "version": "0.0.21-alpha",
  "description": "Render into a three.js canvas using React.",
  "main": "./lib/React3.js",
  "scripts": {
    "compile": "gulp babel",
    "prepublish": "not-in-install && npm run compile || in-install",
    "test": "npm run eslint-internal -loglevel silent && npm run compile -loglevel silent && cd tests && karma start --single-run",
    "eslint-internal": "eslint ./src/ && eslint ./docs/src/ && eslint ./tests/src/",
    "eslint": "npm run eslint-internal -loglevel silent || true",
    "tdd": "bash -c \"(export KARMA_TDD=true; cd tests; node ../node_modules/karma/bin/karma start)\""
  },
  "author": "Firtina \"toxicFork\" Ozbalikci",
  "keywords": "react, three.js",
  "license": "MIT",
  "dependencies": {
    "fbjs": ">=0.7.2"
  },
  "peerDependencies": {
    "react": ">=0.14.5",
    "three": ">=0.74.0"
  },
  "devDependencies": {
    "babel-cli": "^6.5.1",
    "babel-core": "^6.5.1",
    "babel-eslint": "^5.0.0-beta6",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-polyfill": "^6.5.0",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.5.1",
    "del": "^2.2.0",
    "eslint": "^1.10.3",
    "eslint-config-airbnb": "^5.0.0",
    "eslint-plugin-react": "^3.16.1",
    "ghooks": "1.0.3",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "gulp-util": "^3.0.7",
    "in-publish": "^2.0.0",
    "react": "^0.14.7",
    "three": "0.74.0",
    "babel-loader": "^6.2.2",
    "babel-plugin-transform-runtime": "^6.5.0",
    "babel-runtime": "^6.5.0",
    "chai": "^3.5.0",
    "dirty-chai": "^1.2.2",
    "istanbul-instrumenter-loader": "^0.1.3",
    "karma": "^0.13.19",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^0.2.2",
    "karma-coverage": "^0.5.3",
    "karma-firefox-launcher": "^0.1.7",
    "karma-mocha": "^0.2.1",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "0.0.24",
    "karma-webpack": "^1.7.0",
    "mocha": "^2.4.5",
    "mocha-loader": "^0.7.1",
    "react-dom": "^0.14.7",
    "sinon": "2.0.0-pre",
    "source-map-support": "^0.4.0",
    "webpack": "^1.12.13",
    "webpack-dev-server": "^1.14.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/toxicFork/react-three-renderer.git"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-0",
      "react"
    ],
    "plugins": [
      "transform-decorators-legacy"
    ]
  },
  "bugs": {
    "url": "https://github.com/toxicFork/react-three-renderer/issues"
  },
  "homepage": "https://github.com/toxicFork/react-three-renderer#readme",
  "config": {
    "ghooks": {
      "pre-commit": "echo Running eslint before commit && npm run eslint-internal -loglevel silent"
    }
  }
}
